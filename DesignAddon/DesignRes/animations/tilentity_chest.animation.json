{
	"format_version": "1.8.0",
	"animations": {
		"animation.tileentity_chest.invert": {
			"loop": true,
			"bones": {
				"root": {
					"scale": ["variable.mod_invert == 0.0 ? 1.0 : variable.mod_invert", 1.0, 1.0]
				}
			}
		},
		"animation.tileentity_chest.open": {
			"loop": "hold_on_last_frame",
			"animation_length": 0.25,
			"bones": {
				"up": {
					"rotation": {
						"0.0": [0, 0, 0],
						"0.25": [-90, 0, 0]
					}
				}
			}
		},
		"animation.tileentity_chest.close": {
			"loop": "hold_on_last_frame",
			"animation_length": 0.25,
			"bones": {
				"up": {
					"rotation": {
						"0.0": [-90, 0, 0],
						"0.25": [0, 0, 0]
					}
				}
			}
		},
		"animation.tileentity_chest.fix_scale": {
			"loop": true,
			"bones": {
				"root": {
					"position": [-8, 0, 8]
				}
			}
		},
		"animation.tileentity_chest.rotation": {
			"loop": true,
			"bones": {
				"root": {
					"rotation": [0, "variable.mod_rotation * 90", 0]
				}
			}
		}
	}
}